        #if ((defined(__APPLE</strong>) || defined(<strong>OSX</strong>)) &amp;&amp; (defined(<strong>GNUC</strong>) &amp;&amp; (<strong>GNUC</strong> &gt; 2 || (<strong>GNUC</strong> == 2 &amp;&amp; (<strong>GNUC_MINOR</strong> &gt; 95)))))
